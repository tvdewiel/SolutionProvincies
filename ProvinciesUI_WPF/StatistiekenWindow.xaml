<Window x:Class="ProvinciesUI_WPF.StatistiekenWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProvinciesUI_WPF"
        mc:Ignorable="d"
        Title="StatistiekenWindow" Height="450" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Name="TextBoxZip" Margin="5"/>
        <ListBox Grid.Row="1" Name="ListBoxStatistieken" Margin="5"/>
        <Button Grid.Row="2" Content="Sluit venster" Click="ButtonSluitVenster_Click" Margin="5"/>
    </Grid>
</Window>
